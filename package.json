{
  "name": "notion-blog",
  "version": "0.1.0",
  "scripts": {
    "dev": "source .env && next dev",
    "start": "source .env && next start",
    "build": "source .env && next build && node .next/serverless/scripts/build-rss.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "github-slugger": "^1.2.1",
    "next": "^9.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-youtube": "^7.9.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  }
}
